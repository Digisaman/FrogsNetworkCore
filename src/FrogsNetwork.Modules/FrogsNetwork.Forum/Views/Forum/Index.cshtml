@model FrogsNetwork.Forum.ViewModels.ForumsViewModel

@{
    ViewLayout = "Layout__Main";
}




<form asp-controller="FreelancerSearch" asp-action="Index" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" class="form-horizontal no-multisubmit">
    <h1>@T["Forums"]</h1>
    
    <div class="row">
       
        <div class="col">
            <div class="panel-body">
                <table id="assets-data-table" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>CreatedTime</th>
                        </tr>
                    </thead>
                    @foreach (var entry in Model.Forums)
                    {
                        <tr>
                            <td>
                                @entry.Id
                            </td>
                            <td>
                                @entry.Title
                            </td>
                            <td>
                                @entry.Description
                            </td>
                            <td>
                                @entry.CreatedTime
                            </td>
                            
                        </tr>
                    }
                </table>

            </div>
        </div>
        <br />
        <br />
    </div>


</form>